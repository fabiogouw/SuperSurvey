@model SuperSurvey.UseCases.Ports.In.AvailablePollList

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UpdatedAt)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Polls) {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "Detail", "Poll", new { item.Id })
            </td>
        </tr>
}
    </tbody>
</table>
